{
  "name": "solid-jsx",
  "version": "1.0.0",
  "description": "solid-js jsx compatible mdx runtime",
  "main": "./dist/jsx-runtime.js",
  "type": "module",
  "module": "./dist/jsx-runtime.js",
  "exports": {
    ".": {
      "worker": {
        "solid": "./dist/jsx-runtime.js",
        "import": {
          "types": "./dist/jsx-runtime.d.ts",
          "default": "./dist/jsx-runtime.js"
        }
      },
      "browser": {
        "solid": {
          "import": "./dist/jsx-runtime.js"
        },
        "development": {
          "import": {
            "types": "./dist/jsx-runtime.d.ts",
            "default": "./dist/jsx-runtime.js"
          }
        },
        "import": {
          "types": "./dist/jsx-runtime.d.ts",
          "default": "./dist/jsx-runtime.js"
        }
      },
      "deno": {
        "solid": "./dist/jsx-runtime.js",
        "import": {
          "types": "./dist/jsx-runtime.d.ts",
          "default": "./dist/jsx-runtime.js"
        }
      },
      "node": {
        "solid": "./dist/jsx-runtime.js",
        "import": {
          "types": "./dist/jsx-runtime.d.ts",
          "default": "./dist/jsx-runtime.js"
        }
      },
      "solid": {
        "solid": "./dist/jsx-runtime.js"
      },
      "development": {
        "import": {
          "types": "./dist/jsx-runtime.d.ts",
          "default": "./dist/jsx-runtime.js"
        }
      },
      "import": {
        "types": "./dist/jsx-runtime.d.ts",
        "default": "./dist/jsx-runtime.js"
      }
    },
    "./jsx-runtime": "./dist/jsx-runtime.js",
    "./jsx-dev-runtime": "./dist/jsx-runtime.js"
  },
  "types": "./dist/index/index.d.ts",
  "files": [
    "dist",
    "types.d.ts"
  ],
  "sideEffects": false,
  "scripts": {
    "prepare": "husky install",
    "build": "pnpm lint && pnpm pretty && tsup",
    "lint": "pnpm eslint --fix ./src",
    "pretty": "prettier --ignore-path .gitignore --write ./src",
    "prepublishOnly": "pnpm build"
  },
  "engines": {
    "node": ">=16"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/high1/solid-jsx.git"
  },
  "keywords": [
    "solid",
    "mdx"
  ],
  "author": "high1 <high1@mailfence.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/high1/solid-jsx/issues"
  },
  "homepage": "https://github.com/high1/solid-jsx#readme",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^8.9.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-jsonc": "^2.9.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-unicorn": "^48.0.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.1",
    "solid-js": "^1.7.8",
    "tsup": "^7.2.0",
    "typescript": "^5.1.6"
  },
  "lint-staged": {
    "*.{js,ts}": "eslint --fix",
    "*.{js,css,json,md,jsx,yml,yaml}": "prettier --ignore-path .gitignore --write"
  },
  "peerDependencies": {
    "solid-js": "^1.4.0"
  }
}
